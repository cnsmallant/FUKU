@{
    ViewBag.Title = "首页-福库商城";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
<style>
    .tal { width: 650px; border-top: 1px solid red; border-right: 1px solid red; float: left; overflow: hidden; }

    .th { width: 100%; height: auto; border-bottom: 1px solid red; float: left; overflow: hidden; }

    .td { width: 100px; height: auto; float: left; border-left: 1px solid red; }
</style>
<div id="slideBox" class="slideBox">
    <div class="container">
        <dl id="pro_nav">
            <dt><span>&nbsp;</span>全部商品分类</dt>
            @foreach (var item in ViewBag.list)
            {
                <dd><a href="@Url.Action("Index", "Goods", new { GoodsClassId = @item.pro_class_page })"><span>&nbsp;</span>@item.pro_class_name<i>&nbsp;</i></a></dd>
            }
        </dl>
    </div>
    <div class="hd">
        <ul>
            @foreach (var item in ViewBag.bannerlist)
            {
                <li></li>
            }
        </ul>
    </div>
    <div class="bd">
        <ul>
            @foreach (var item in ViewBag.bannerlist)
            {

                <a href="@item.com_banner_url" target="_blank">
                    <li>
                        <div style="background: url(@item.com_img_src) no-repeat center;">
                        </div>
                    </li>
                </a>

            }

        </ul>
    </div>
    <a class="prev" href="javascript:void(0)"></a>
    <a class="next" href="javascript:void(0)"></a>
</div>
<script id="jsID" type="text/javascript">
    var ary = location.href.split("&");
    jQuery(".slideBox").slide({
        mainCell: ".bd ul",
        effect: "left",
        autoPlay: true,
        mouseOverStop: false,
        interTime: 4000,
    });
</script>
<div id="recom">
    <div class="container">


        @foreach (var item in ViewBag.recommend)
        {
            <div class="recom_box">
                <a href="@Url.Action("GoodsDetails", "Goods", new { id = @item.pro_sku_code })">
                    <img src="@item.com_img_src" />
                    <div class="proInfo">
                        <p class="f18">@item.pro_brand_id</p>
                        <p class="f13">&yen;@item.pro_skuitem_price</p>
                    </div>
                    <span>荐</span>
