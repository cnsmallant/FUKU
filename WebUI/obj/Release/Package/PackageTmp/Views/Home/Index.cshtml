    var dkp1 = $("#dkp1");
    var dsh1 = $("#dsh1");
    yzj.show();
    ztj.hide();
    dkp.hide();
    dsh.hide();
    yzj1.show();
    ztj1.hide();
    dkp1.hide();
    dsh1.hide();
    var ayzj = $("#ayzj");
    var aztj = $("#aztj");
    var adkp = $("#adkp");
    var adsh = $("#adsh");
    ayzj.click(function () {

        yzj.show();
        ztj.hide();
        dkp.hide();
        dsh.hide();
        yzj1.show();
        ztj1.hide();
        dkp1.hide();
        dsh1.hide();
    });
    aztj.click(function () {

        yzj.hide();
        ztj.show();
        dkp.hide();
        dsh.hide();
        yzj1.hide();
        ztj1.show();
        dkp1.hide();
        dsh1.hide();
    });
    adkp.click(function () {

        yzj.hide();
        ztj.hide();
        dkp.show();
        dsh.hide();
        yzj1.hide();
        ztj1.hide();
        dkp1.show();
        dsh1.hide();
    });
    adsh.click(function () {

        yzj.hide();
        ztj.hide();
        dkp.hide();
        dsh.show();
        yzj1.hide();
        ztj1.hide();
        dkp1.hide();
        dsh1.show();
    });
</script>
