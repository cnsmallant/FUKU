<style>
    #tab01 .tdHead {
        border-top: 1px solid #d7d7d7;
        border-left: 1px solid #d7d7d7;
        border-right: 1px solid #d7d7d7;
    }

    #tab01 tr td {
        border: none;
        border-bottom: 1px solid #e0e0e0;
        height: 78px;
        text-indent: 0;
    }

    #tab01 .tdHead td {
        border-bottom: 1px solid #d7d7d7;
        color: #e70012;
        font-size: 14px;
    }

        #tab01 .tdHead td span {
            color: #5a5a5a;
            font-size: 12px;
        }

    #tab01 tr .maijia {
        color: #e70012;
        width: 310px;
        line-height: 78px;
    }

        #tab01 tr .maijia img {
            height: 55px;
            width: 55px;
            border-radius: 27px;
            margin-right: 10px;
            margin-top: 13px;
            float: left;
            margin-left: 13px;
        }

    .col03 {
        width: 280px;
    }

    #tab01 tr .color {
        text-align: left;
        text-indent: 0;
    }

    .order {
        table-layout: auto;
    }

    .orderState a {
        color: #515151;
        display: block;
    }

    .buyPro img {
        height: 80px;
    }

    .buyPro a {
        color: #696969;
        display: block;
        text-indent: 0;
        line-height: 80px;
        float: left;
    }

    .order tr td {
        border: 1px solid #dbdee9;
        border-collapse: collapse;
        font-size: 14px;
        color: #696969;
        text-indent: 25px;
        padding-top: 10px;
        padding-bottom: 10px;
    }

    .orderDetail {
        height: 34px !important;
        color: #696969 !important;
        padding: 0 !important;
    }

        .orderDetail span {
            margin-right: 20px;
        }

    .order #tab_head td {
        text-indent: 0px !important;
    }

    .orderAction a {
        color: #515151 !important;
    }

    .order .buyPro {
        width: 350px;
    }

        .order .buyPro span {
            display: block;
            width: auto;
            text-align: left;
        }

    .pinglun_title {
        font-size: 14px;
        font-weight: bold;
        width: 100%;
        margin-bottom: 5px;
        text-indent: 20px;
    }

    .pinglun_time {
        font-size: 13px;
        width: 100%;
        margin-bottom: 5px;
        text-indent: 20px;
    }

    .pinglun_text {
        color: #e70012;
        font-size: 13px;
        width: 90%;
        margin-left: 20px;
        text-indent: 0em;
    }

    .orderAction span {
        color: #e70012;
    }

    #tab_head tr td {
    }
</style>
