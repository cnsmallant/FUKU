@{
    ViewBag.Title = "";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<div class="container-fluid" style="margin-top:50px;margin-bottom:60px;">
    <div class="row">

        <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">

            <!-- Wrapper for slides -->
            <div class="carousel-inner" role="listbox">
                @{
                    for (int i = 0; i < ViewBag.bannerlist.Count; i++)
                    {
                        if (i == 0)
                        {
                            <div class="item active">
                                <a href="@ViewBag.bannerlist[i].com_banner_url">
                                    <img src="http://www.cuckooshop.cn/@ViewBag.bannerlist[i].com_img_src" class="img-responsive">
                                </a>
                            </div>
                        }
                        else
                        {
                            <div class="item">
                                <a href="@ViewBag.bannerlist[i].com_banner_url">
                                    <img src="http://www.cuckooshop.cn/@ViewBag.bannerlist[i].com_img_src" class="img-responsive">
                                </a>
                            </div>
                        }
                    }
                }
            </div>
        </div>


    </div>
    <div class="row">
        <div class="thumbnail content_thumbnail" style="color:#717171;padding-top:10px;">
            <a href="@Url.Action("Index", "Personal")">
                <div class="col-xs-3 text-center"><p><img src="/img/001.png" class="img-responsive" /></p><p>我的</p></div>
            </a>
            <a href="@Url.Action("GoodsNews", "Goods")">
                <div class="col-xs-3 text-center"><p><img src="/img/news.png" class="img-responsive" /></p><p>新品</p></div>
            </a>
            <a href="@Url.Action("GoodsSales", "Goods")">
                <div class="col-xs-3 text-center"><p><img src="/img/hots.png" class="img-responsive" /></p><p>促销</p></div>
            </a>
            <a href="@Url.Action("Index", "ShoppingGuide" , new { classid="process" })">
                <div class="col-xs-3 text-center"><p><img src="/img/004.png" class="img-responsive" /></p><p>指南</p></div>
            </a>
        </div>
    </div>

    <div class="row content_thumbnail" style="color:#717171;padding-top:10px;">
        @foreach (var item in ViewBag.recommend)
        {
            <a href="@Url.Action("GoodsDetails", "Goods" , new { id=@item.pro_sku_code })" style="color:#242221">
                <div class="col-xs-6 text-center" style="padding-left:3px;padding-right:3px; position:relative">
                    <p><img src="http://www.cuckooshop.cn/@item.com_img_src" class="img-responsive" /></p>
                    <p style="position:absolute;left:5px;top:5px;">@item.pro_brand_id</p>
                    <p style="position:absolute;left:5px;top:20px;">&yen;<span>@item.pro_skuitem_price</span></p>
                    <span style="position:absolute;right:4px;top:-2px; background-color:#e60012; color:#ffffff;padding:2px;border-bottom-left-radius:10px;border-bottom-right-radius:10px;">荐</span>
                </div>
            </a>
        }


    </div>
    <div class="row content_thumbnail" style="background-color:transparent;">
        <div class="page-header">
            <div class=" content_thumbnail div_pager">
                <span class="pull-left"><span class="span_tags">|</span> <span class="title_text">超高压电饭煲系列</span></span>
                <span class="pull-right"><a href="@Url.Action("Index", "Goods", new { GoodsClassId = "RiceCooker"})"> <span class="fa fa-plus-square span_plus"></span> </a></span>
            </div>
        </div>
        <div class=" content_thumbnail" style="background-color:transparent; margin-bottom:0px;">
